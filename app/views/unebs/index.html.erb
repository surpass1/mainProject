<h3>Listing unebs</h3>

<table class="table table-striped" style="font-size:14px;">
  <tr>
    <th>Academic year</th>
    <th>Subject</th>
    <th>Academic level</th>    
    <th>Action</th>
  </tr>

<% @unebs.each do |uneb| %>
  <tr>
    <td><%= uneb.academic_year %></td>
    <td><%= uneb.subject %></td>
    <td><%= uneb.academic_level %></td>
    <td><%= link_to 'Show', uneb , :class=>"btn-link"%> |
        <%= link_to('Edit', {:action=>'edit', :id=>uneb.id}, :class=>"btn-link") %> |
        <%= link_to 'Destroy', uneb, confirm: 'Are you sure?', method: :delete, :class=>"btn-link" %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Uneb', new_uneb_path , :class=>"btn btn-medium btn-info"%>
